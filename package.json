{
  "scripts": {
    "dev:frontend": "npm run dev --prefix frontend -- --open",
    "build:frontend": "npm run build --prefix frontend ",

    "dev:backend": "npm run develop --prefix backend -- --open",
    "build:backend": "npm run build --prefix backend ",
    
    "dev:json-server": "json-server test-backend/db.json --port 3001 -- --open",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:json-server\"",
    "build": "npm run build --prefix frontend && npm run build --prefix backend"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "json-server": "^0.17.4"
  }
}
